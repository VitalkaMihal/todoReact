#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/tanyu/Desktop/it_incubator/рабочая/todo/todoReact/node_modules/.pnpm/vite-node@2.1.8/node_modules/vite-node/node_modules:/mnt/c/Users/tanyu/Desktop/it_incubator/рабочая/todo/todoReact/node_modules/.pnpm/vite-node@2.1.8/node_modules:/mnt/c/Users/tanyu/Desktop/it_incubator/рабочая/todo/todoReact/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/tanyu/Desktop/it_incubator/рабочая/todo/todoReact/node_modules/.pnpm/vite-node@2.1.8/node_modules/vite-node/node_modules:/mnt/c/Users/tanyu/Desktop/it_incubator/рабочая/todo/todoReact/node_modules/.pnpm/vite-node@2.1.8/node_modules:/mnt/c/Users/tanyu/Desktop/it_incubator/рабочая/todo/todoReact/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vite-node.mjs" "$@"
else
  exec node  "$basedir/../../vite-node.mjs" "$@"
fi
